    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch test-malloc.exe",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test-malloc.exe",
            "args": ["R', "/etc/passwd", "cat", "-n"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch test-mymalloc.exe",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test-mymalloc.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
     ]
